{
  "name": "course-enrollment-system",
  "version": "1.0.0",
  "description": "A MEAN stack course enrollment system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && ng serve",
    "build": "cd client && ng build",
    "install-client": "cd client && npm install",
    "seed": "node seedData.js",
    "setup": "node setup.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "mean",
    "course",
    "enrollment",
    "education"
  ],
